{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
	"name": "postgres-operator",
	"annotations": {
		"cloud.google.com/load-balancer-type": "Internal"
	},
        "labels": {
            "name": "postgres-operator"
        }
    },
    "spec": {
        "ports": [{
            "name": "apiserver",
            "protocol": "TCP",
            "port": 8443,
            "targetPort": 8443
        }],
        "selector": {
            "name": "postgres-operator"
        },
        "type": "LoadBalancer",
        "sessionAffinity": "None"
    }
}
